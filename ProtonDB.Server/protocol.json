{
  "Request": {
    "Format": [
      {
        "Command": "FETCH | EXIT | PROFILE",
        "Data": null
      },
      {
        "Command": "QUERY",
        "Data": "string"
      },
      {
        "Command": "DEBUG",
        "Data": true | false
      },
      {
        "Command": "LOGIN",
        "Data": "username,password"
      }
    ]
  },
  "Response": {
    "Format": {
      "Status": "ok | error",
      "Message": "string",
      "Result": [ "string", "..." ]
    }
  },
  "Encoding": "UTF-8",
  "Transport": "TCP (one JSON line per message)"
}
